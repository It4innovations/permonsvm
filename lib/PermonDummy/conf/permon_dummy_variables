# -*- mode: makefile -*-

-include ${PERMON_DIR}/lib/permon/conf/permon_variables

PERMON_PKG_PREFIX           = PERMON_DUMMY
PERMON_DUMMY_NAME           = permondummy
PERMON_DUMMY_MAIN_HEADER    = include/permondummy.h
PERMON_DUMMY_LIB_DIR        = ${PERMON_DUMMY_DIR}/${PETSC_ARCH}/lib
PERMON_DUMMY_INCLUDE        = -I${PERMON_DUMMY_DIR}/include -I${PERMON_DUMMY_DIR}/${PETSC_ARCH}/include
PERMON_DUMMY_CC_INCLUDES    = ${PERMON_DUMMY_INCLUDE} ${PERMON_CC_INCLUDES}
PERMON_DUMMY_FC_INCLUDES    = ${PERMON_DUMMY_INCLUDE} ${PERMON_FC_INCLUDES}
PERMON_DUMMY_CCPPFLAGS      = ${PERMON_DUMMY_INCLUDE} ${PERMON_CCPPFLAGS}
PERMON_DUMMY_FCPPFLAGS      = ${PERMON_DUMMY_INCLUDE} ${PERMON_FCPPFLAGS}
PERMON_DUMMY_C_SH_LIB_PATH  = ${CC_LINKER_SLFLAG}${PERMON_DUMMY_LIB_DIR}
PERMON_DUMMY_F_SH_LIB_PATH  = ${FC_LINKER_SLFLAG}${PERMON_DUMMY_LIB_DIR}
PERMON_DUMMY_DEP_LIBS       = ${PERMON_LIB}

